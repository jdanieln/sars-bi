                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
	Cat.CategoryID, Prd.ProductID, Sup.SupplierID, Cus.CustomerID, 
	MONTH(Ord.OrderDate) as MES, YEAR(Ord.OrderDate) as ANIO,
	SUM(Det.Quantity * Det.UnitPrice) as MONTO
FROM Orders as Ord
INNER JOIN [Order Details] as Det on Ord.OrderID = Det.OrderID
INNER JOIN Products as Prd on Det.ProductID = Prd.ProductID
INNER JOIN Categories as Cat on Prd.CategoryID = Cat.CategoryID
INNER JOIN Suppliers as Sup on Prd.SupplierID = Sup.SupplierID
INNER JOIN Customers as Cus on Ord.CustomerID = Cus.CustomerID
